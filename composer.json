{
    "require": {
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.49",
        "nesbot/carbon": "^3.0",
        "pestphp/pest": "^2.33",
        "ramsey/uuid": "^4.7",
        "innmind/immutable": "^4.14"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "Advent\\": "advent_of_craft/",
            "Katas\\": "katas_coding/",
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.6"
    }
}
